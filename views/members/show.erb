<h2>Name: <%= @member.name %></h2>
<div class="member_holder">
<img src="/images/<%= @member.pic_url %>" alt="member-photo" >
</div>
<h2>Membership Type: <%= @member.membership_type %></h2>
<h2>Registration Date: <%= @member.registration_date %></h2>


<h2>Classes</h2>
<% if @classes.first != nil %>
<% @classes.each do |fclass|%>
<p><%= fclass.type.capitalize %> | <%=fclass.time.slice(0..4)  %> | <%=fclass.date_as_written  %></p>
<% end %>
<% else %>
<p><%= @member.name %> hasn't booked any classes yet.</p>
<% end %>



<form action="/members/<%= @member.id %>/delete" method="POST"><input type="submit" value="Delete"></form>
<form action="/members/<%= @member.id%>/edit" method="GET"><input type="submit" value="Edit"></form>
